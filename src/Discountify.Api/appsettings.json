{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DiscountifyContextConnection": "Server=(localdb)\\MSSQLLocalDb;Database=DiscountifyDb;Trusted_Connection=true;MultipleActiveResultSets=true;"
  },
  "IdentityServer": {
    "Authority": "http://localhost:5000",
    "RequireHttpsMetadata": false,
    "ApiName": "DiscountifyWebApi"
  },
  "AllowedCorsOrigins": {
    "Collection": [ "http://localhost:8080" ]
  }
}
